<%= form_with model: @inn, class:"row" do |f| %>
  <div class="col-md-4 mb-3">
    <%= f.label :trade_name, class: "form-label" %>
    <%= f.text_field :trade_name, class: "form-control" %>
  </div>
  <div class="col-md-4 mb-3">
    <%= f.label :corporate_name, class: "form-label" %>
    <%= f.text_field :corporate_name, class: "form-control" %>
  </div>
  <div class="col-md-4 mb-3">
    <%= f.label :cnpj, class: "form-label" %>
    <%= f.text_field :cnpj, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :phone, class: "form-label" %>
    <%= f.text_field :phone, class: "form-control" %>
  </div>
  <div class="col-md-3 mb-3">
    <%= f.label :email, class: "form-label" %>
    <%= f.text_field :email, class: "form-control" %>
  </div>
  <div class="col-md-4 mb-3">
    <%= f.label :address, class: "form-label" %>
    <%= f.text_field :address, class: "form-control" %>
  </div>
  <div class="col-md-1 mb-3">
    <%= f.label :address_number, class: "form-label" %>
    <%= f.text_field :address_number, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :neighborhood, class: "form-label" %>
    <%= f.text_field :neighborhood, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :state, class: "form-label" %>
    <%= f.text_field :state, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :city, class: "form-label" %>
    <%= f.text_field :city, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :cep, class: "form-label" %>
    <%= f.text_field :cep, class: "form-control" %>
  </div>
  <div class="col-md-3 mb-3">
    <%= f.label :payment_methods, class: "form-label" %>
    <%= f.text_field :payment_methods, class: "form-control" %>
  </div>
  <div class="col-md-2 mt-4 mb-3">
    <%= f.label :pets_allowed, class: "form-label" %>
    <%= f.check_box :pets_allowed, class: "form-check-input" %>
  </div>
  <div class="col-md-6 mb-3">
    <%= f.label :description, class: "form-label" %>
    <%= f.text_area :description, class: "form-control" %>
  </div>
  <div class="col-md-6 mb-3">
    <%= f.label :polices, class: "form-label" %>
    <%= f.text_area :polices, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :checkin_time, class: "form-label" %>
    <%= f.time_field :checkin_time, class: "form-control" %>
  </div>
  <div class="col-md-2 mb-3">
    <%= f.label :checkout_time, class: "form-label" %>
    <%= f.time_field :checkout_time, class: "form-control" %>
  </div>
  <div class="col-md-2 mt- mb-3">
    <%= f.label :active, class: "form-label" %> <br>
    <%= f.radio_button :active, true, class: "btn-check", id:"success-outlined" %>
    <%= f.label "#{t(:true)}", class: "btn btn-outline-success", for:"success-outlined" %>
    <%= f.radio_button :active, false, class: "btn-check", id:"danger-outlined" %>
    <%= f.label "#{t(:false)}", class: "btn btn-outline-danger", for:"danger-outlined" %>
  </div>
  <div class="mb-3">
    <%= f.submit nil, class: "btn btn-primary" %>
  </div>
<% end %>